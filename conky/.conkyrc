out_to_x no
own_window no
out_to_console yes
background no
max_text_width 0
update_interval 1.0
total_run_times 0
short_units on
if_up_strictness address
use_spacer left
override_utf8_locale no
cpu_avg_samples 1

TEXT
[{ "full_text" : " Battery: $battery_bar $battery_time " , "color" : "\#ffff00" },
{ "full_text" : " $time " , "color" : "\#ffff00" }],
